  "bugs": {
    "url": "https://github.com/Yu-HuanWu/TagAlongs/issues"
  },
  "homepage": "https://github.com/Yu-HuanWu/TagAlongs#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^6.3.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.10",
    "node-sass": "^6.0.1",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "validator": "^13.6.0"
  },
